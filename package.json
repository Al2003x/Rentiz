{
  "name": "template-htmlacademy",
  "private": true,
  "description": "Сборка на основе сборки HTMLAcademy",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.0.0",
    "autoprefixer": "10.4.0",
    "browser-sync": "2.27.7",
    "del": "6.0.0",
    "gulp": "4.0.2",
    "gulp-cheerio": "^1.0.0",
    "gulp-dart-sass": "^1.0.2",
    "gulp-file-include": "^2.3.0",
    "gulp-htmlmin": "5.0.1",
    "gulp-libsquoosh": "^1.0.12",
    "gulp-plumber": "1.2.1",
    "gulp-postcss": "9.0.1",
    "gulp-rename": "2.0.0",
    "gulp-svgmin": "^4.1.0",
    "gulp-svgstore": "8.0.0",
    "gulp-terser": "2.1.0",
    "postcss": "8.4.4",
    "postcss-csso": "5.0.1",
    "postcss-scss": "^4.0.3",
    "stylelint": "^14.5.3",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-order": "^5.0.0"
  },
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"src/sass/**/*.scss\" --custom-syntax postcss-scss",
    "test": "npm run editorconfig && npm run stylelint",
    "build": "gulp build",
    "start": "gulp"
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not ie <= 11",
    "not op_mini all",
    "not < 0.25%"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "src/*.html",
    "src/js/**/*.js",
    "src/img/**/*.svg",
    "src/sass/**/*.scss"
  ],
  "engines": {
    "node": "^16"
  },
  "type": "module"
}
